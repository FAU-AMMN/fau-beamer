%%% Footline %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\setbeamertemplate{footline}
{%
\nointerlineskip%
\begin{beamercolorbox}[wd=\paperwidth,ht=\fau@outer@urlraum,dp=0pt]{footline}
%{\normalsize\textbf{\fau@url}}%
\vbox to\dimexpr\fau@outer@urlraum{%
\vfil%
\hbox to \dimexpr\paperwidth{
    \hskip \dimexpr\fau@outer@absenderraum+\fau@outer@linienstaerke
	{\normalsize\textbf{\fau@url}}%
	\hfill%
    {\normalsize\textbf{\fau@signature}}
    \hspace{\beamer@rightmargin}
}%
\vfil%
}%
\end{beamercolorbox}%
}

