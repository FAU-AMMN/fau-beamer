%%% Frametitle %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\setbeamertemplate{frametitle}
{%
\nointerlineskip%
\begin{beamercolorbox}[wd=\dimexpr\paperwidth-\fau@outer@absenderraum-\fau@outer@linienstaerke,
					   ht=\dimexpr\fau@outer@absenderhoehe,
					   dp=0pt]{frametitle}%
\begin{minipage}[b][\dimexpr\fau@outer@absenderhoehe-\fau@outer@linienstaerke]%-\fau@outer@absenderraum]
				[b]{\dimexpr\paperwidth-2\fau@outer@absenderraum-\fau@outer@linienstaerke}
\vbox to\dimexpr\fau@outer@absenderhoehe-\fau@outer@linienstaerke{%
\vskip\beamer@leftmargin%
\hbox to \dimexpr\paperwidth-2\fau@outer@absenderraum{
	\hskip\beamer@leftmargin
	\ifx\insertauthor\empty\else
		\Large\textbf{\insertauthor}%\newline
	\fi\hfill}%
\hbox to \dimexpr\paperwidth-2\fau@outer@absenderraum{
	\hskip\beamer@leftmargin
	\ifx\insertinstitute\empty\else
		\Large\textmd\insertinstitute%\newline
	\fi\hfill}%
\vfil%
\hbox to \dimexpr\paperwidth-2\fau@outer@absenderraum{
	\hskip\beamer@leftmargin
	{\usebeamerfont{title}\inserttitle}
	\hfill}%
\hbox to \dimexpr\paperwidth-2\fau@outer@absenderraum{
	\hskip\beamer@leftmargin
	\ifx\insertsubtitle\empty\else
		{\usebeamerfont{subtitle}\insertsubtitle}
	\fi\hfill}%
\vfil%
\vskip\beamer@leftmargin%
}%
\end{minipage}%
\end{beamercolorbox}%
\nointerlineskip
% ----------------------------------------------------------------------------------------	
% Seperation line
% ----------------------------------------------------------------------------------------
\begin{beamercolorbox}[wd=\dimexpr\paperwidth-\fau@outer@absenderraum-\fau@outer@linienstaerke,
					   ht=\fau@outer@linienstaerke,
					   dp=0pt]{headline}%
\end{beamercolorbox}%
}
